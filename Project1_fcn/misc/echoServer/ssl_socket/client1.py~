from __future__ import print_function
import socket
import ssl

clientSocket=socket.socket(socket.AF_INET,socket.SOCK_STREAM)
sslSocket = ssl.wrap_socket(clientSocket, keyfile=None, certfile=None, server_side=False, cert_reqs=CERT_NONE)
server='localhost'
port=8000

sslSocket.connect((server,port))


while 1:
	data=raw_input('Client: ')
	sslSocket.write(data)
	if not data:break
	newdata=sslSocket.read(1024)
	print('Server: ',newdata)
sslSocket.close()
